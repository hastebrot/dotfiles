// Place your key bindings in this file to overwrite the defaults
[
{ "key": "ctrl+7",         "command": "editor.action.commentLine",
                              "when": "editorTextFocus && !editorReadonly" },
{ "key": "ctrl+shift+7",   "command": "editor.action.blockComment",
                              "when": "editorTextFocus && !editorReadonly" },
{ "key": "f2",             "command": "workbench.action.quickOpen" },
{ "key": "alt+1",          "command": "workbench.action.toggleSidebarVisibility" },
{ "key": "alt+2",          "command": "workbench.action.output.toggleOutput" },
{ "key": "alt+3",          "command": "workbench.action.togglePanel" },
{ "key": "alt+4",          "command": "workbench.view.explorer" },
{ "key": "alt+5",          "command": "workbench.view.git" },
{ "key": "alt+6",          "command": "workbench.view.extensions" },
{ "key": "ctrl+q",         "command": "editor.action.rename",
                              "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly" },
{ "key": "ctrl+k",         "command": "workbench.action.nextEditor" },
{ "key": "ctrl+j",         "command": "workbench.action.previousEditor" },
{ "key": "ctrl+shift+k",   "command": "workbench.action.moveEditorToNextGroup" },
{ "key": "ctrl+shift+j",   "command": "workbench.action.moveEditorToPreviousGroup" },
{ "key": "ctrl+o",         "command": "workbench.action.navigateBack" },
{ "key": "ctrl+i",         "command": "workbench.action.navigateForward" },
{ "key": "ctrl+shift+enter", "command": "clojureVSCode.evalAndShowResult" },
{ "key": "ctrl+f11",       "command": "workbench.action.toggleZenMode" }
]